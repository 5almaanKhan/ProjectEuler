public class MyClass {
    public static void main(String args[]) {
        int sum = 0;
        int max = 1000;
        for(int i = 3; i < max; i+=3) {
                sum+= i;
        }
        for(int i = 5; i < max; i+=5) {
            if(i % 3 != 0) {
                sum+= i;
            }
        }
        System.out.println(sum);
    }
}
